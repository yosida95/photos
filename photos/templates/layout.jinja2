{#- vim:set filetype=htmljinja shiftwidth=2: -#}

<!DOCTYPE html>
<html lang='ja'>
  <head prefix="og: http://ogp.me/ns#">
    <meta charset="UTF-8">
    <base href="https://yosida95.com/">
    <meta name="description" content="{% block description %}{% endblock %}">
    <!-- The Open Graph protocol -->
    <meta property="og:title" content="{{ self.title() }} &mdash; Photos &mdash; yosida95">
    <meta property="og:type" content="article">
    <meta property="og:url" content="{{ request.url }}">
    <meta property="og:description" content="{{ self.description() }}">
    <meta property="og:image" content="https://yosida95.com/_static/icon_200x200.png">
    <meta property="og:site_name" content="yosida95">
    <!-- Twitter Cards -->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@yosida95">
    <meta name="twitter:title" content="{{ self.title() }} &mdash; Photos &mdash; yosida95">
    <meta name="twitter:description" content="{{ self.description() }}">
    <meta name="twitter:image" content="https://yosida95.com/_static/icon_200x200.png">
    {%- block ext_meta -%}
    {%- endblock %}

    <title>{% block title %}{% endblock %} &mdash; Photos &mdash; yosida95</title>

    <!-- Website Styles -->
    <link rel="stylesheet" type="text/css" href="/_static/css/style.css" media="screen">
    <!-- Responsive & Retina Display Stylesheets -->
    <link rel="stylesheet" type="text/css" href="/_static/css/responsive.css" media="screen">
    <link rel="stylesheet" type="text/css" href="/_static/css/retina.css" media="screen">
    <!-- Custom Stylesheet -->
    <link rel="stylesheet" type="text/css" href="/_static/css/custom.css" media="screen">
    <!--[if IE 7]>
    <link rel="stylesheet" type="text/css" href="_static/css/ie7.css" media="screen">
    <![endif]-->

    <link rel="alternate" type="application/rss+xml" href="/rss.xml">
    <link rel="shortcut icon" href="_static/favicon.ico">

    <script type="text/javascript">
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-15957452-1', 'auto');
      ga('send', 'pageview');
    </script>
    <script type="text/javascript" src="/_static/twitter.js"></script>
  </head>
  <body>
    <div id="wrapper">
      <div id="header">
        <div class="inner">
          <div class="logo">
            <a href="pages/about.html"><strong>yosida95</strong></a>
          </div>
          <div class="nav">
            <ul>
              <li><a href="/pages/about.html"><span>Home</span></a></li>
              <li><a href="/page1.html"><span>Blog</span></a></li>
              <li><a href="/photos"><span>Photos</span></a></li>
            </ul>
          </div><!-- .nav end -->
          <div class="clear"></div>
        </div><!-- .inner end -->
      </div><!-- #header end -->

      <div id="main">
        <div class="tagline inner">
          <h1>Photos</h1>
          <span>将来の夢はおもしろお兄さんです。</span>
          <div class="clear"></div>
        </div>
        <div class="inner">
          {%- block content -%}
          {%- endblock %}
        </div><!-- .inner END -->
      </div><!-- #main End -->

      <div id="footer">
        <div class="footer_small">
          <div class="copyright">&copy; Copyright 2015, Kohei YOSHIDA.</div>
          <div class="social">
            <a href="https://github.com/yosida95"><img src="_static/img/social/github.svg" width="24" alt="GitHub"></a>
            <a href="https://twitter.com/yosida95"><img src="_static/img/social/twitter.svg" width="24" alt="Twitter"></a>
            <a href="https://www.facebook.com/yosida95"><img src="_static/img/social/facebook.svg" width="24" alt="facebook"></a>
            <a href="rss.xml"><img src="_static/img/social/rss.svg" width="24" alt="RSS Feed"></a>
          </div>
        </div>
      </div><!-- #footer End -->
    </div><!-- #wrapper End -->
  </body>
</html>
